<?php

/**
 * @file
 */

/**
 * Implements hook_theme().
 */
function react4drupal_theme($existing, $type, $theme, $path) {
  return [
    'react4drupal' => [
      'variables' => [],
    ],
  ];
}

/**
 * Implements hook_preprocess_HOOK().
 */
function react4drupal_preprocess_node(&$variables) {
  $variables['content']['react']['#theme'] = 'react4drupal';
}
